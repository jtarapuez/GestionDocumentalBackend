{
  "endpoint": "GET /api/v1/inventarios/pendientes-aprobacion",
  "description": "Lista inventarios pendientes de aprobación (para supervisores)",
  "method": "GET",
  "url": "http://localhost:8080/api/v1/inventarios/pendientes-aprobacion",
  "headers": {
    "Content-Type": "application/json",
    "Accept": "application/json"
  },
  "note": "Este endpoint requiere rol SUPERVISOR_SDNGD (actualmente temporal hasta implementar Keycloak)",
  "expectedResponse": {
    "statusCode": 200,
    "body": {
      "data": [
        {
          "id": 1,
          "idSeccion": 23,
          "idSerie": 7,
          "idSubserie": 5,
          "numeroExpediente": "EXP-TEST-001",
