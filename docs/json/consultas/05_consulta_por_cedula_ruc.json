{
  "endpoint": "POST /api/v1/consultas",
  "description": "Consulta inventarios por cédula o RUC del beneficiario",
  "method": "POST",
  "url": "http://localhost:8080/api/v1/consultas",
  "headers": {
    "Content-Type": "application/json",
    "Accept": "application/json"
  },
  "body": {
    "numeroCedula": "1234567890"
  },
  "examples": [
    {
      "description": "Búsqueda por cédula (persona natural)",
      "body": {
        "numeroCedula": "1234567890"
      }
    },
    {
      "description": "Búsqueda por RUC (empresa)",
      "body": {
        "numeroRuc": "0991234567001"
      }
    },
    {
      "description": "Búsqueda por cédula con estado específico",
      "body": {
        "numeroCedula": "1234567890",
        "estado": "Aprobado"
      }
    }
  ],
  "note": "⚠️ Este filtro puede estar marcado como TODO en el código",
  "curlExample": "curl -X POST 'http://localhost:8080/api/v1/consultas' -H 'Content-Type: application/json' -H 'Accept: application/json' -d '{\"numeroCedula\":\"1234567890\"}'"
}
